@model IEnumerable<FoodYeah.Model.Costumer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Costumer_Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CostumerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CostumerAge)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Costumer_Category.Costumer_CategoryDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CostumerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CostumerAge)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CostumerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CostumerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CostumerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
